    "reverse": {
      "type": "boolean",
      "default": false
    },
    "images_overlap": {
      "type": "boolean",
      "default": false
    },
    "image_fade": {
      "type": "enumeration",
      "required": true,
      "default": "none",
      "enum": [
        "top",
        "bottom",
