{
  "collectionName": "components_ui_icons",
  "info": {
    "displayName": "Icon"
  },
  "options": {},
  "attributes": {
    "icon_name": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "is_image"
            },
            false
          ]
        }
      }
    },
    "image": {
      "type": "media",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "is_image"
            },
            true
          ]
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "is_image": {
      "type": "boolean",
      "default": true
    }
  },
  "config": {}
}
