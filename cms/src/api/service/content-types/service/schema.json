{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "subservices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subservice.subservice"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "icon": {
      "type": "component",
      "component": "ui.icon",
      "repeatable": false,
      "required": true
    },
    "seo": {
      "type": "component",
      "component": "seo.seo",
      "repeatable": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
