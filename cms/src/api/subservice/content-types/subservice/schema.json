{
  "kind": "collectionType",
  "collectionName": "subservices",
  "info": {
    "singularName": "subservice",
    "pluralName": "subservices",
    "displayName": "Subservice"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "addon_in_project": {
      "type": "boolean",
      "default": false
    },
    "price_once": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "addon_in_project"
            },
            true
          ]
        }
      }
    },
    "price_month": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "addon_in_project"
            },
            true
          ]
        }
      }
    },
    "content": {
      "type": "blocks"
    },
    "hero": {
      "type": "component",
      "component": "block.hero",
      "repeatable": false
    },
    "introduction": {
      "type": "component",
      "component": "sections.introduction",
      "repeatable": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "block.list",
        "block.info",
        "block.full-section"
      ]
    }
  }
}
