{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "Start sida"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "hero": {
      "type": "component",
      "component": "block.hero",
      "repeatable": false,
      "required": true
    },
    "has_form": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "hero_form": {
      "type": "component",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "has_form"
            },
            true
          ]
        }
      },
      "component": "form.form",
      "repeatable": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "block.featured-services",
        "block.list",
        "block.featured-offers",
        "block.full-section",
        "block.featured-projects",
        "block.faq",
        "block.featured-articles"
      ]
    }
  }
}
